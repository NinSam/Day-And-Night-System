{
	"geode": "4.3.1",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "ninsam.day_and_night_system",
	"name": "Day & Night System",
	"version": "v1.1.7",
	"developer": "NinSam",
	"description": "Adds a day and night system in the GD menus!",
	"early-load": true,
	"dependencies": {

		"geode.node-ids": {
			"version": "1.20.0",
            "importance": "required"
		}

	},
	"tags":["offline","interface"],
	"resources": {
		"files":["resources/*.png"]
    },
	"links": {
		"source": "https://github.com/NinSam/Day-And-Night-System"
	},
	"settings":{

		"time-settings":{
			"name": "Customize Time",
			"type": "title"
		},
		"start-sunrise-hr":{
			"name": "Start Sunrise Hr",
			"type": "int",
			"default": 7,
			"min": 0,
			"max": 24,
			"description": "12am-12pm or 0:00-12:00 (0-12). 1pm-12am or 13:00-24:00 (13-24).",
			"enable-if": "enable-sunrise"
		},
		"start-sunrise-min":{
			"name": "Start Sunrise Min",
			"type": "int",
			"default": 0,
			"min": 0,
			"max": 59,
			"description": "0-59 = minutes",
			"enable-if": "enable-sunrise"
		},
		"end-sunrise-hr":{
			"name": "End Sunrise Hr",
			"type": "int",
			"default": 8,
			"min": 0,
			"max": 24,
			"description": "12am-12pm or 0:00-12:00 (0-12). 1pm-12am or 13:00-24:00 (13-24).",
			"enable-if": "enable-sunrise"
		},
		"end-sunrise-min":{
			"name": "End Sunrise Min",
			"type": "int",
			"default": 0,
			"min": 0,
			"max": 59,
			"description": "0-59 = minutes",
			"enable-if": "enable-sunrise"
		},
		"start-sunset-hr":{
			"name": "Start Sunset Hr",
			"type": "int",
			"default": 18,
			"min": 0,
			"max": 24,
			"description": "12am-12pm or 0:00-12:00 (0-12). 1pm-12am or 13:00-24:00 (13-24).",
			"enable-if": "enable-sunset"
		},
		"start-sunset-min":{
			"name": "Start Sunset Min",
			"type": "int",
			"default": 0,
			"min": 0,
			"max": 59,
			"description": "0-59 = minutes",
			"enable-if": "enable-sunset"
		},
		"end-sunset-hr":{
			"name": "End Sunset Hr",
			"type": "int",
			"default": 19,
			"min": 0,
			"max": 24,
			"description": "12am-12pm or 0:00-12:00 (0-12). 1pm-12am or 13:00-24:00 (13-24).",
			"enable-if": "enable-sunset"
		},
		"end-sunset-min":{
			"name": "End Sunset Min",
			"type": "int",
			"default": 0,
			"min": 0,
			"max": 59,
			"description": "0-59 = minutes",
			"enable-if": "enable-sunset"
		},
		"start-night-hr":{
			"name": "Start Night Hr",
			"type": "int",
			"default": 19,
			"min": 0,
			"max": 24,
			"description": "12am-12pm or 0:00-12:00 (0-12). 1pm-12am or 13:00-24:00 (13-24).",
			"enable-if": "enable-night"
		},
		"start-night-min":{
			"name": "Start Night Min",
			"type": "int",
			"default": 0,
			"min": 0,
			"max": 59,
			"description": "0-59 = minutes",
			"enable-if": "enable-night"
		},
		"end-night-hr":{
			"name": "End Night Hr",
			"type": "int",
			"default": 21,
			"min": 0,
			"max": 24,
			"description": "12am-12pm or 0:00-12:00 (0-12). 1pm-12am or 13:00-24:00 (13-24).",
			"enable-if": "enable-night"
		},
		"end-night-min":{
			"name": "End Night Min",
			"type": "int",
			"default": 0,
			"min": 0,
			"max": 59,
			"description": "0-59 = minutes",
			"enable-if": "enable-night"
		},
		"start-dark-hr":{
			"name": "Start Dark Hr",
			"type": "int",
			"default": 21,
			"min": 0,
			"max": 24,
			"description": "12am-12pm or 0:00-12:00 (0-12). 1pm-12am or 13:00-24:00 (13-24).",
			"enable-if": "enable-dark"
		},
		"start-dark-min":{
			"name": "Start Dark Min",
			"type": "int",
			"default": 0,
			"min": 0,
			"max": 59,
			"description": "0-59 = minutes",
			"enable-if": "enable-dark"
		},
		"end-dark-hr":{
			"name": "End Dark Hr",
			"type": "int",
			"default": 7,
			"min": 0,
			"max": 24,
			"description": "12am-12pm or 0:00-12:00 (0-12). 1pm-12am or 13:00-24:00 (13-24).",
			"enable-if": "enable-dark"
		},
		"end-dark-min":{
			"name": "End Dark Min",
			"type": "int",
			"default": 0,
			"min": 0,
			"max": 59,
			"description": "0-59 = minutes",
			"enable-if": "enable-dark"
		},
		"main-settings":{
			"name": "Events",
			"type": "title"
		},
		"enable-sunrise":{
			"name": "Sunrise Event",
			"description": "Enable/Disable the sunrise event.",
			"type": "bool",
			"default": true
		},
		"enable-sunset":{
			"name": "Sunset Event",
			"description": "Enable/Disable the sunset event.",
			"type": "bool",
			"default": true
		},
		"enable-night":{
			"name": "Night Event",
			"description": "Enable/Disable the night event.",
			"type": "bool",
			"default": true
		},
		"enable-dark":{
			"name": "Dark Event",
			"description": "Enable/Disable the dark event.",
			"type": "bool",
			"default": true
		},
		"stars-settings":{
			"name": "Stars",
			"type": "title"
		},
		"opacity-small-stars-night":{
			"name": "Small Star Opacity In Night Event",
			"description": "Set the opacity of the small stars during the night event.",
			"type": "int",
			"default": 220,
			"min": 0,
			"max": 255
		},
		"opacity-big-stars-night":{
			"name": "Big Star Opacity In Night Event",
			"description": "Set the opacity of the big stars during the night event.",
			"type": "int",
			"default": 255,
			"min": 0,
			"max": 255
		},
		"opacity-small-stars-dark":{
			"name": "Small Star Opacity In Dark Event",
			"description": "Set the opacity of the small stars during the dark event.",
			"type": "int",
			"default": 220,
			"min": 0,
			"max": 255
		},
		"opacity-big-stars-dark":{
			"name": "Big Star Opacity In Dark Event",
			"description": "Set the opacity of the big stars during the dark event.",
			"type": "int",
			"default": 255,
			"min": 0,
			"max": 255
		},
		"overlay-settings":{
			"name": "Screen Overlay",
			"type": "title"
		},
		"opacity-dark-overlay-night":{
			"name": "Dark Overlay Opacity In Night Event",
			"description": "Set the opacity of the dark overlay during the night event.",
			"type": "int",
			"default": 105,
			"min": 0,
			"max": 225
		},
		"opacity-dark-overlay-dark":{
			"name": "Dark Overlay Opacity In Dark Event",
			"description": "Set the opacity of the dark overlay during the dark event.",
			"type": "int",
			"default": 105,
			"min": 0,
			"max": 225
		}
	}
}